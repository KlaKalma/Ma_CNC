<pyvcp>

    <labelframe text="EtherCAT Master">
        <hbox>
            <label><text>"Master State:"</text></label>
            <led><halpin>"master-state-init"</halpin><on_color>"orange"</on_color><off_color>"grey"</off_color><size>15</size></led>
            <label><text>"Init"</text></label>
            <led><halpin>"master-state-preop"</halpin><on_color>"yellow"</on_color><off_color>"grey"</off_color><size>15</size></led>
            <label><text>"PreOP"</text></label>
            <led><halpin>"master-state-safeop"</halpin><on_color>"cyan"</on_color><off_color>"grey"</off_color><size>15</size></led>
            <label><text>"SafeOP"</text></label>
            <led><halpin>"master-state-op"</halpin><on_color>"green"</on_color><off_color>"grey"</off_color><size>15</size></led>
            <label><text>"OP"</text></label>
        </hbox>
    </labelframe>
	
	<labelframe text="EL1008">
		<hbox>
		<label>
			<text>"Air OK "</text>
		</label>
		<led>
			<halpin>"air-ok"</halpin>
		</led>
		</hbox>

	</labelframe>
	
    <labelframe text="EL2008">
		<button>
			<halpin>"dout0"</halpin>
			<text>"Relay 1"</text>
		</button>
		<button>
			<halpin>"dout1"</halpin>
			<text>"Relay 2"</text>
		</button>
		<button>
			<halpin>"toolchange"</halpin>
			<text>"Tool change"</text>
		</button>
    </labelframe>
    
    <labelframe text="Servo LC10E - Axe X">
        <hbox>
            <label><text>"Pos Cmd:"</text></label>
            <number><halpin>"x-pos-cmd"</halpin><font>("Helvetica",12)</font><width>10</width><format>"+4.3f"</format></number>
            <label><text>" mm    Pos FB:"</text></label>
            <number><halpin>"x-pos-fb"</halpin><font>("Helvetica",12)</font><width>10</width><format>"+4.3f"</format></number>
            <label><text>" mm"</text></label>
        </hbox>
        <hbox>
            <label><text>"Erreur:"</text></label>
            <number><halpin>"x-f-error"</halpin><font>("Helvetica",12,"bold")</font><width>10</width><format>"+4.4f"</format></number>
            <label><text>" mm"</text></label>
        </hbox>
        <hbox>
            <label><text>"Vit Cmd:"</text></label>
            <number><halpin>"x-velocity"</halpin><font>("Helvetica",12)</font><width>8</width><format>"+4.1f"</format></number>
            <label><text>" mm/s  Vit FB:"</text></label>
            <number><halpin>"x-velocity-fb"</halpin><font>("Helvetica",12)</font><width>8</width><format>"+4.1f"</format></number>
            <label><text>" mm/s"</text></label>
        </hbox>
        <hbox>
            <label><text>"Err. Vit:"</text></label>
            <number><halpin>"x-vel-error"</halpin><font>("Helvetica",12,"bold")</font><width>8</width><format>"+4.2f"</format></number>
            <label><text>" mm/s  RPM:"</text></label>
            <number><halpin>"x-motor-rpm"</halpin><font>("Helvetica",12)</font><width>6</width><format>"+5.0f"</format></number>
            <label><text>" rpm"</text></label>
        </hbox>
        <hbox>
            <vbox>
                <label><text>"Charge Moteur"</text></label>
                <bar>
                    <halpin>"x-torque-bar"</halpin>
                    <min_>0</min_>
                    <max_>300</max_>
                    <bgcolor>"grey"</bgcolor>
                    <fillcolor>"green"</fillcolor>
                </bar>
                <hbox>
                    <label><text>"Couple:"</text></label>
                    <number><halpin>"x-torque-pct"</halpin><font>("Helvetica",11,"bold")</font><width>6</width><format>"+5.1f"</format></number>
                    <label><text>" %"</text></label>
                </hbox>
            </vbox>
            <vbox>
                <button>
                    <halpin>"x-fault-reset"</halpin>
                    <text>"RESET"</text>
                </button>
            </vbox>
        </hbox>
        <hbox>
            <label><text>"Min:"</text></label>
            <led><halpin>"x-limit-min"</halpin><on_color>"red"</on_color><off_color>"grey"</off_color><size>20</size></led>
            <label><text>" Homed:"</text></label>
            <led><halpin>"x-homed"</halpin><on_color>"green"</on_color><off_color>"grey"</off_color><size>20</size></led>
            <label><text>" Max:"</text></label>
            <led><halpin>"x-limit-max"</halpin><on_color>"red"</on_color><off_color>"grey"</off_color><size>20</size></led>
        </hbox>
        <hbox>
            <label><text>"Enabled:"</text></label>
            <led><halpin>"x-enabled"</halpin><on_color>"green"</on_color><off_color>"red"</off_color><size>20</size></led>
            <label><text>" Fault:"</text></label>
            <led><halpin>"x-fault"</halpin><on_color>"red"</on_color><off_color>"grey"</off_color><size>20</size></led>
            <label><text>" Warn:"</text></label>
            <led><halpin>"x-warning"</halpin><on_color>"yellow"</on_color><off_color>"grey"</off_color><size>20</size></led>
        </hbox>
        <hbox>
            <label><text>"État:"</text></label>
            <led><halpin>"x-state-init"</halpin><on_color>"orange"</on_color><off_color>"grey"</off_color><size>15</size></led>
            <label><text>"Init"</text></label>
            <led><halpin>"x-state-preop"</halpin><on_color>"yellow"</on_color><off_color>"grey"</off_color><size>15</size></led>
            <label><text>"PreOP"</text></label>
            <led><halpin>"x-state-safeop"</halpin><on_color>"cyan"</on_color><off_color>"grey"</off_color><size>15</size></led>
            <label><text>"SafeOP"</text></label>
            <led><halpin>"x-state-op"</halpin><on_color>"green"</on_color><off_color>"grey"</off_color><size>15</size></led>
            <label><text>"OP"</text></label>
        </hbox>
    </labelframe>
    
